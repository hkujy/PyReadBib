@article{ WOS:000462189100001,
Author = {Chow, Andy H. F. and Li, Shuai},
Title = {{Modelling and managing bus service regularity with influence of
   prevailing traffic}},
Journal = {{TRANSPORTMETRICA B-TRANSPORT DYNAMICS}},
Year = {{2019}},
Volume = {{7}},
Number = {{1}},
Pages = {{82-106}},
Month = {{DEC 23}},
Abstract = {{This paper presents a multi-modal framework for modelling and analysing
   different headway control strategies for improving bus service
   regularity. The interaction between buses and their surrounding traffic
   is captured in a multi-modal system through a Hamilton-Jacobi
   formulation of kinematic wave model. This paper further presents a set
   of signal-based strategies which regulate bus headway through adjusting
   signal timing plans. The results reveal that the capability of
   regulating bus headway disturbances through utilising traffic signals is
   important for maximising bus service regularity and coordinating the bus
   dynamics with surrounding traffic. This study generates new insights on
   managing bus reliability and multi-modal traffic in busy urban networks.}},
