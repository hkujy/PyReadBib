@article{ WOS:000241411500018,
Author = {Bellei, Giuseppe and Gentile, Guido and Meschini, Lorenzo and Papola,
   Natale},
Title = {{A demand model with departure time choice for within-day dynamic traffic
   assignment}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2006}},
Volume = {{175}},
Number = {{3}},
Pages = {{1557-1576}},
