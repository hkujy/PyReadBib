
@article{ WOS:000489086800011,
Author = {Zhang, Xiang and Rey, David and Waller, S. Travis and Chen, Nathan},
Title = {{Range-Constrained Traffic Assignment with Multi-Modal Recharge for
   Electric Vehicles}},
Journal = {{NETWORKS \& SPATIAL ECONOMICS}},
Year = {{2019}},
