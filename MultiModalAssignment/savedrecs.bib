@article{ WOS:000361772700011,
Author = {Zangui, Mahmood and Aashtiani, Hedayat Z. and Lawphongpanich, Siriphong
   and Yin, Yafeng},
Title = {{Path-differentiated pricing in congestion mitigation}},
Journal = {{TRANSPORTATION RESEARCH PART B-METHODOLOGICAL}},
Year = {{2015}},
Volume = {{80}},
Pages = {{202-219}},
Month = {{OCT}},
Abstract = {{Instead of charging tolls on individual links, this paper considers
   doing the same on paths. Path and link tolls are ``valid{''} if they
   encourage motorists to use routes that collectively lead to a target
   distribution, e.g., one that minimizes travel delay. Because the numbers
   of valid link and path tolls are typically infinite, an objective in
   pricing tolls is to find a set of valid tolls that yields the least
   revenue to lessen the financial burden on motorists.
   Path tolls are generally more flexible than link tolls and this paper
   shows that this flexibility can substantially reduce the financial
   burden on motorists. Additionally, valid path tolls yielding the least
   revenue possess characteristics with interesting policy implications. To
   determine these path tolls, it is natural to formulate the problem as a
   mathematical program with complementarity constraints. However, this
   paper also investigates alternative formulations that highlight the
   problem's complexity and suggest ways to solve the problem efficiently.
