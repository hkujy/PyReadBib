@article{ WOS:000659033600003,
Author = {Ryu, Seungkyu and Chen, Anthony and Kitthamkesorn, Songyot},
Title = {{A Two-Phase Gradient Projection Algorithm for Solving the Combined Modal
   Split and Traffic Assignment Problem with Nested Logit Function}},
Journal = {{JOURNAL OF ADVANCED TRANSPORTATION}},
Year = {{2021}},
Volume = {{2021}},
Month = {{APR 19}},
Abstract = {{This study provides a gradient projection (GP) algorithm to solve the
   combined modal split and traffic assignment (CMSTA) problem. The nested
   logit (NL) model is used to consider the mode correlation under the user
   equilibrium (UE) route choice condition. Specifically, a two-phase GP
   algorithm is developed to handle the hierarchical structure of the NL
   model in the CMSTA problem. The Seoul transportation network in Korea is
   adopted to demonstrate an applicability in a large-scale multimodal
   transportation network. The results show that the proposed GP solution
   algorithm outperforms the method of the successive averages (MSA)
   algorithm and the classical Evan's algorithm.}},
