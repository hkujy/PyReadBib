@inproceedings{ WOS:000614113600019,
Author = {Hu Wen-jun and Zhou Xi-zhao},
Book-Group-Author = {{IEEE}},
Title = {{A Multi-mode Two-Level Transportation Model Based On Network
   Vulnerability}},
